{
  "manifest_version": 3,
  "name": "Simple Tab",
  "version": "5.3",
  "description": "极简导航 - 快速访问",
  "permissions": [
    "storage"
  ],
  "optional_host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}